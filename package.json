{
  "name": "owp",
  "version": "1.0.0",
  "description": "sample project on nodejs and angular",
  "main": "index.js",
    "engines": {
      "node": "10.16.3",
      "npm": "6.9.0"
    },
  "scripts": {
    "start": "node index",
    "server": "nodemon index",
    "client-install": "npm install --prefix owp-client",
    "client": "npm run start --prefix owp-client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm run client-install && npm run build --prefix owp-client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vsymoniuk/owp.git"
  },
  "keywords": [
    "nodejs",
    "angular",
    "course",
    "work"
  ],
  "author": "vsymoniuk",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vsymoniuk/owp/issues"
  },
  "homepage": "https://github.com/vsymoniuk/owp#readme",
  "dependencies": { 
    "body-parser": "^1.19.0",
    "concurrently": "^5.0.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "moment": "^2.24.0",
    "mongoose": "^5.7.4",
    "morgan": "^1.9.1", 
    "multer": "^1.4.2",
    "nodemon": "^1.19.3"
  }
}